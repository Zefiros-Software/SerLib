[
    {
        "project": "SerLib",
        "options": {
            "HeaderOnly": false
        },
        "do":
        [
            {"kind": "StaticLib"},
            {"language": "C++"},
            {
                "filter-options": {
                    "HeaderOnly": false
                },
                "do":
                [
                    {"defines": "SERIALISATION_NO_HEADER_ONLY"},
                    {"files": [ "serialisation/src" ] }
                ],
                "otherwise":
                [                    
                    {"headeronly": true}
                ]
            },
            {"export":[
                {"includedirs": "serialisation/include/"}                     
            ]}
        ]
    }
]    